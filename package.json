{
  "name": "clarity-desk",
  "version": "1.0.0",
  "description": "Smart Productivity Dashboard with Google Meet Integration",
  "homepage": "https://navaneeth-nair.github.io/Code-fest-2025-Hackathon",
  "scripts": {
    "start": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "start:frontend": "cd frontend && npm start",
    "start:backend": "cd backend && npm start",
    "start:dev": "concurrently \"npm run dev:backend\" \"npm run start:frontend\"",
    "dev:backend": "cd backend && npm run dev",
    "build": "cd frontend && npm run build",
    "build:frontend": "cd frontend && npm run build",
    "install:all": "npm install && cd frontend && npm install && cd ../backend && npm install",
    "deploy": "cd frontend && npm run build && npx gh-pages -d build",
    "predeploy": "npm run build",
    "test": "cd frontend && npm test",
    "clean": "rimraf frontend/build frontend/node_modules backend/node_modules node_modules"
  },
  "dependencies": {
    "react-router-dom": "^7.9.1"
  },
  "devDependencies": {
    "gh-pages": "^6.1.1",
    "concurrently": "^8.2.2",
    "rimraf": "^5.0.5"
  }
}
